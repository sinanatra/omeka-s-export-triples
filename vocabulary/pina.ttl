pina:frameImprovementNeeds a rdf:Property ;
    rdfs:label "Sicherungsbedarf (Frame Improvement Needs)" ;
    rdfs:comment "Sicherungsbedarf: anbringen, verbessern, ergänzen (Improvement needs: attach, improve, ad" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

