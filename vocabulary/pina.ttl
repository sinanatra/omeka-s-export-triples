@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix o: <http://omeka.org/s/vocabs/o#> .
@prefix pina: <http://omeka.localhost:8080/ns/pina/> .

<http://omeka.localhost:8080/ns/pina/> a owl:Ontology ;
    dcterms:title "pina" ;
    dcterms:description "A custom ontology for the entries from the Lipperheidesche Kostümbibliothek (Lipperheide Costume Library) with approx. 600 paintings and miniatures on the history of fashion and costume from the period 1450(1900." .

pina:paintingPrecautionaryMeasure a rdf:Property ;
    rdfs:label "Andere Sicherungsmaßnahmen Gemälde (Painting Precautionary Measure)" ;
    rdfs:comment "Additional adhesives and precautionary measures on the painting" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

pina:framePrecautionaryMeasure a rdf:Property ;
    rdfs:label "Andere Sicherungsmaßnahmen Rahmen (Frame Precautionary Measure)" ;
    rdfs:comment "Additional adhesives and precautionary measures on the frame" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

pina:paintingRequirementNote a rdf:Property ;
    rdfs:label "Anmerkung Gemälde Bedarf (Painting Requirement Note)" ;
    rdfs:comment "Notes on restauration  and concervation requirements" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

pina:frameRequirementNote a rdf:Property ;
    rdfs:label "Anmerkung Rahme Bedarf (Frame Requirement Note)" ;
    rdfs:comment "Notes on the restauration and concervation process" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

pina:objectStorage a rdf:Property ;
    rdfs:label "Art der Lagerung (Type of storage)" ;
    rdfs:comment "Information regarding the storage of the object" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

pina:labelTraces a rdf:Property ;
    rdfs:label "Etikettspuren (Label Traces)" ;
    rdfs:comment "Existence of traces of adhesive tape (former label) on bottom left" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

pina:externalResearchBy a rdf:Property ;
